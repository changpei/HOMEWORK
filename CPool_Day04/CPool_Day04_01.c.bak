#include <stdio.h> 
void my_swap(int *p,int *q);
int main(void)
{
    int a,b,*p=&a,*q=&b;
    printf("请输入两个整数：");
    scanf("%d%d",&a,&b);
    printf("交换前a=%d,b=%d\n",a,b);
    my_swap(p,q);
    printf("交换后a=%d,b=%d\n",a,b);
    return 0;
}
void my_swap(int *p,int *q)
{
    int temp;
    temp=*q;
    *q=*p;
    *p=temp;
}